{
	"format_version": "1.16.0",
	"minecraft:entity": {
		"description": {
			"identifier": "dest:objective",
			"is_spawnable": true,
			"is_summonable": true,
			"is_experimental": false
		},
		"component_groups": {
			"dest:despawn": {
				"minecraft:instant_despawn": {}
			}
		},
		"components": {
			"minecraft:is_hidden_when_invisible": {},
			"minecraft:nameable": {
				"always_show": true,
				"allow_name_tag_renaming": false
			},
			"minecraft:type_family": {
				"family": [
					"objective"
				]
			},
			"minecraft:collision_box": {
				"width": 1.0,
				"height": 1.0
			},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"on_damage": {
							"filters": {
								"test": "is_family",
								"subject": "other",
								"value": "player"
							},
							"event": "dest:warning"
						},
						"deals_damage": false
					}
				]
			},
			"minecraft:health": {
				"value": 250,
				"max": 250
			},
			"minecraft:scale": {
				"value": 1.0
			},
			"minecraft:knockback_resistance": {
				"value": 1.0
			},
			"minecraft:tick_world": {
				"never_despawn": true,
				"radius": 2
			},
			"minecraft:physics": {
				"has_gravity": false
			},
			"minecraft:pushable": {
				"is_pushable": false,
				"is_pushable_by_piston": true
			}
		},
		"events": {
			"dest:warning": {},
			"dest:despawn": {
				"add": {
					"component_groups": [
						"dest:despawn"
					]
				}
			}
		}
	}
}